{
	"Commands": [
		{
			"Key": "md5sum",
			"Command": "/usr/bin/md5sum",
			"Options": ["%file"]
		},
		{
			"Key": "avprobe",
			"Command": "/usr/local/bin/avprobe",
			"Options": ["%file", "-show_streams", "-show_format", "-of", "json"]
		},
		{
			"Key": "rsync",
			"Command": "/usr/bin/rsync",
			"Options": ["-avz", "%src", "%dest"]
		},
		{
			"Key": "cp",
			"Command": "/bin/cp",
			"Options": ["%src", "%dest"]
		}
	]
}
